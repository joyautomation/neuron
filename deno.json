    "date-fns": "npm:date-fns@^3.6.0",
    "long": "npm:long@^5.2.3",
    "mqtt": "npm:mqtt@^5.10.1",
    "pako": "npm:pako@^2.1.0",
    "ramda": "npm:ramda@^0.30.1",
    "sparkplug-payload": "npm:sparkplug-payload@^1.0.3"
  }
